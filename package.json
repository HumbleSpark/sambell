{
  "name": "sambell",
  "version": "0.5.2",
  "description": "create a universal, single page app with a single file",
  "main": "build/app/index.js",
  "scripts": {
    "clean": "rimraf build",
    "start": "babel ./src --watch -d build",
    "build": "babel ./src -d build --ignore '__tests__'",
    "gulp": "gulp --gulpfile=./build/webpack/gulpfile.js",
    "prepublish": "npm run clean && npm run build"
